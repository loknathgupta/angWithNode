        <!--Logo Area Starts-->
        <% include ../layout/logo-section %>
            <!--Logo Area Ends-->
            <!--Left side Menu Starts-->
            <% include ../layout/left-menu %>
                <!--Left Menu Ends-->
    </div>
    <!--Left Side Ends-->
    

            <!--Header Right Menu starts-->
            <% include ../layout/header-right-menu %>
                <!--Header Right Menu Ends-->

        </div>
        <!--Right Side Header Ends-->

        <!--Content Wrapper Starts-->
        <div class="content-wrapper">
            
            <!-- Drag and Drog Functionality Div Starts-->
            <div class="sortable">
                <div class="col-md-12">
                    <!--editable starts-->
                    <div class="panel">
                        <div class="panel-heading">
                            Revenue Report
                                  
                                <div class="clearfix add-new pull-right" id="dataTableButtons">
                    
                                </div>
                                
                        </div>

                        <div class="panel-body">
                                <div class="alert alert-danger fade in hidden">
                                        <a href="#" class="close alert-close" >&times;</a>
                                        <strong>Error!</strong> <span id="msg-div-error"></span>
                                    </div>
                                <div class="alert alert-success fade in hidden">
                                        <a href="#" class="close alert-close" >&times;</a>
                                        <strong>Success!</strong> <span id="msg-div"></span>
                                       
                                    </div>
                                    <% include ../elements/accountm_filters %>
                            <form method="POST" id="lst-frm">
                           
                            
                            <table class="table table-striped table-hover table-bordered display dataTable no-footer" id="am-table"  >
                                <thead>
                                    <tr role="row">
                                        <th class="sorting" rowspan="2" >Client </th>
                                        <th class="sorting" rowspan="2" width="15%" >PM</th>
                                        <th class="sorting" rowspan="2"  width="10%" >AM</th>
                                        <th class="sorting" rowspan="2"  width="10%" >AO</th>
                                        <th class="sorting" rowspan="2"  width="15%" >Pricing Head</th>  
                                        <th class="sorting" rowspan="2"  width="5%" >Vol. in <span id="past-month"><%=lastMonth%></span></th>                                     
                                        <th class="sorting" rowspan="2"  width="5%" >MTD Vol.</th>
                                        <th class="sorting text-center" width="15%" colspan="3" >Vol. on </th> 
                                          
                                        <th class="sorting" rowspan="2"  width="5%">CCY</th>
                                        <th class="sorting" rowspan="2"  width="5%" >U/M</th>
                                        <th class="sorting" rowspan="2"  width="5%" >Price</th>                                 
                                        <th class="sorting" rowspan="2"  width="5%">Amount</th>                            
                                    </tr>
                                    <tr role="row">
                                        <th width="5%"><%=thirdLastDate%></th>
                                        <th width="5%"><%=secondLastDate%></th>
                                        <th width="5%"><%=lastDate%></th>
                                    </tr>
                                </thead>
                                <tbody>                     
                                </tbody>
                            </table>
                            
                            <div class="total-ammount pull-right">
                                <h5>Total Amount By Currency</h5>
                                    <table class="table table-striped table-hover table-bordered display dataTable no-footer"  id="ccy-total" >
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting" width="30%" >Currency </th>
                                                    <th class="sorting"  >Total</th>
                                                                          
                                                </tr>
                                            </thead>
                                            <tbody>                     
                                            </tbody>
                                        </table> 
                            </div>
</form>
                        </div>
                    </div>
                </div>
                <!--Editable Table Ends-->

            </div>
        </div>
        <!-- Drag and Drog Functionality Div Starts-->
    </div>
